{
  "name": "@swan-io/css",
  "version": "0.1.4",
  "license": "MIT",
  "description": "A lightweight and performant atomic CSS-in-JS library",
  "author": "Mathieu Acthernoene <mathieu.acthernoene@swan.io>",
  "homepage": "https://github.com/swan-io/css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swan-io/css.git"
  },
  "packageManager": "pnpm@10.14.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./vite-plugin": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.mjs",
      "default": "./dist/vite-plugin.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "css",
    "css-in-js",
    "react",
    "style",
    "typescript"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "format": "prettier '**/*' --ignore-unknown --write",
    "clean": "rm -rf dist",
    "test:ci": "playwright install --with-deps --only-shell chromium && CI=true vitest run",
    "test": "vitest run",
    "test:webkit": "BROWSER=webkit vitest run",
    "test:firefox": "BROWSER=firefox vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "build": "pnpm clean && tsup",
    "prepack": "prettier '**/*' -u -c && pnpm typecheck && pnpm test && pnpm build"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ]
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "peerDependencies": {
    "vite": ">=5"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "@emotion/hash": "^0.9.2",
    "@react-native/normalize-colors": "^0.81.0",
    "csstype": "^3.1.3",
    "magic-string": "^0.30.17",
    "oxc-parser": "^0.82.1",
    "oxc-walker": "^0.4.0",
    "postcss-value-parser": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitest/browser": "^3.2.4",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.1"
  }
}
